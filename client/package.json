{
  "name": "rpanel",
  "version": "0.1.0",
  "description": "rloop ui",
  "main": "electron-main.js",
  "scripts": {
    "postinstall": "./node_modules/.bin/electron-rebuild",
    "bundle-renderer-js": "./node_modules/webpack/bin/webpack.js",
    "start": "npm run bundle-renderer-js && ./node_modules/electron-prebuilt/cli.js electron-main.js",
    "package": "npm run bundle-renderer-js && ./node_modules/electron-packager/cli.js --platform=darwin --arch=x64 --prune --overwrite .",
    "__dev-serve-renderer-js": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --colors --inline",
    "dev": "./node_modules/concurrently/src/main.js -k 'npm run __dev-serve-renderer-js' 'env ENV=dev ./node_modules/electron-prebuilt/cli.js electron-main.js'"
  },
  "author": "Ted Suzman",
  "license": "MIT",
  "dependencies": {
    "highcharts": "4.2.5",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "underscore": "1.8.3",
    "zmq": "2.15.2"
  },
  "devDependencies": {
    "babel-core": "6.9.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "concurrently": "2.1.0",
    "css-loader": "0.23.1",
    "electron-prebuilt": "1.2.0",
    "electron-rebuild": "1.1.5",
    "file-loader": "0.8.5",
    "json-loader": "0.5.4",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  }
}
